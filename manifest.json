{
  "manifest_version": 3,
  "name": "Email Writer",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "main.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}